<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentRandomCat">

    <fragment
        android:id="@+id/fragmentSearch"
        android:name="com.msidorenko.cat_project.ui.fragments.FragmentSearch"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" >
        <action
            android:id="@+id/action_fragmentSearch_to_fragmentBreedInfo"
            app:destination="@id/fragmentBreedInfo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentRandomCat"
        android:name="com.msidorenko.cat_project.ui.fragments.FragmentRandomCat"
        android:label="fragment_randomV2"
        tools:layout="@layout/fragment_random" >
        <action
            android:id="@+id/action_fragmentRandomCat_to_fragmentWebView"
            app:destination="@id/fragmentWebView" />
        <action
            android:id="@+id/action_fragmentRandomCat_to_fragmentBreedInfo"
            app:destination="@id/fragmentBreedInfo" />
    </fragment>
    <fragment
        android:id="@+id/fragmentBreedInfo"
        android:name="com.msidorenko.cat_project.ui.fragments.FragmentBreedInfo"
        android:label="FragmentBreedInfo" >
        <action
            android:id="@+id/action_fragmentBreedInfo_to_fragmentWebView"
            app:destination="@id/fragmentWebView" />
        <argument
            android:name="breedNumber"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/fragmentWebView"
        android:name="com.msidorenko.cat_project.ui.fragments.FragmentWebView"
        android:label="fragment_webview"
        tools:layout="@layout/fragment_webview" >
        <argument
            android:name="url"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/fragmentLiked"
        android:name="com.msidorenko.cat_project.ui.fragments.FragmentLiked"
        android:label="fragment_liked"
        tools:layout="@layout/fragment_liked" >
        <action
            android:id="@+id/action_fragmentLiked_to_fragmentBreedInfo"
            app:destination="@id/fragmentBreedInfo" />
        <action
            android:id="@+id/action_fragmentLiked_to_fragmentWebView"
            app:destination="@id/fragmentWebView" />
    </fragment>
</navigation>